@font-face
  font-family: 'FontAwesome'
  src: url('./fonts/fa-solid-900.eot')
  src: url('./fonts/fa-solid-900.eot?#iefix') format('embedded-opentype'),
  url('./fonts/fa-regular-400.woff') format('woff'),
  url('./fonts/fa-regular-400.ttf') format('truetype')
  font-weight: normal
  font-style: normal

@font-face
  font-family: 'Myriad Pro Regular'
  src: url('./fonts/MyriadPro-Regular.eot')
  src: url('./fonts/MyriadPro-Regular.eot?#iefix') format('embedded-opentype'),
  url('./fonts/MyriadPro-Regular.woff') format('woff'),
  url('./fonts/MyriadPro-Regular.ttf') format('truetype')
  font-weight: normal
  font-style: normal

@font-face
  font-family: 'Myriad Pro Bold'
  src: url('./fonts/MyriadPro-Bold.eot')
  src: url('./fonts/MyriadPro-Bold.eot?#iefix') format('embedded-opentype'),
  url('./fonts/MyriadPro-Bold.woff') format('woff'),
  url('./fonts/MyriadPro-Bold.ttf') format('truetype')
  font-weight: normal
  font-style: normal

@font-face
  font-family: 'Myriad Pro Light SemiExtended'
  src: url('./fonts/MyriadPro-LightSemiExt.eot')
  src: url('./fonts/MyriadPro-LightSemiExt.eot?#iefix') format('embedded-opentype'),
  url('./fonts/MyriadPro-LightSemiExt.woff') format('woff'),
  url('./fonts/MyriadPro-LightSemiExt.ttf') format('truetype')
  font-weight: normal
  font-style: normal

body
 background-color: #ffffff
 color: #000000
 font-family: 'Myriad Pro Regular'
 font-size: 14px
 max-width:1920px
 min-width: 600px

.header
  text-align: center

.round
  background: #ffffff
  border: 1px solid #3d6cb0
  border-radius: 50%
  position: relative
  margin: 0 auto

.round::after
  content:""
  display: block
  position: absolute
  background:#3d6cb0
  border-radius: 50%
  top: 10px
  left: 10px

// @media (min-width: 1280px) and (max-width:1366px)
  .round
    width: 238px
    height: 238px
  .round::after
    width: 208px
    height: 208px
    top: 15px
    left: 15px
  .name
    font-size: 54px
  .surname
    font-size: 64px
  .profession
    font-size: 25.78px
  .about-header
    font-size: 42.47px
  .about-info
    font-size: 15.92px
  span.profile-info-bold
    font-size: 15.92px
    line-height: 17px
  .contact-info-header
    font-size: 21.23px
    color: #3d6cb0
  .contact-info-data
    font-size: 21.23px
  .info-header
    font-size: 26.54px
    text-transform: uppercase
  .experience-description
    font-size: 15.92px
  .experience-period
    font-size: 26.54px
  .col-1
    width:555px
  .col-2
    width:770px

/* start margin-top for headers*/
.profile
  margin-top: 78px
.contact
  margin-top: 48px
.skills
  margin-top: 26px
  margin-left: -2px

/* end margin-top for headers*/

/*start icons*/
.profile-icon
  background: url('../img/icons.png') no-repeat -64px -1px
.contact-icon
  background: url('../img/icons.png') no-repeat -128px -1px
.skills-icon
  background: url('../img/icons.png') no-repeat -197px -1px
.education-icon
  background: url('../img/icons.png') no-repeat 3px -1px
.experience-icon
  background: url('../img/icons.png') no-repeat -333px -1px
.software-icon
  background: url('../img/icons.png') no-repeat -265px -1px
/*end icons*/
.name
  font-family: 'Myriad Pro Light SemiExtended'
  text-transform: uppercase
  margin-top:26px
  letter-spacing: 0.5px
.surname
  text-transform: uppercase
  font-family: 'Myriad Pro Bold'
  margin-top: -16px
.profession
  text-transform: uppercase
  font-family: 'Myriad Pro Bold'
  letter-spacing: -0.5px
.col-1 .about
  padding-left: 56px
  padding-right: 74px
.col-1,
.col-2
  padding-top: 10px

.block-header
  width: 100%
  position: relative

.about-icon
  display:inline-block
  float: left
  width: 73px
  height: 73px
  border: 1px solid #0071bb
  border-radius: 50%
  position: relative
  margin: 0 auto
  background-color:#0071bb
  box-shadow: 0px 0px 0px 3px #fff inset

.header
  width: 100%
.profile .content
  margin-top: 12px
  margin-left: -1px
.content
  display: inline-block
.software .content,
.contact .content
  margin-top: 90px
.skills .content
  margin-top: 82px
.col-1
  margin-top:73px
  display: inline-block
  float:left

.col-2
  margin-top:60px
  display: inline-block
  float:right
.skills .content
  text-align: center
  margin-left: -68px
table
  margin-left: -79px
  margin-top: 2px
td
  margin:0
  padding:0
  height: 54px
  text-align: left
td.contact-info-header
   width: 131px
.skill
  margin-left: 8px
  margin-top: 17px
.skill-name.info-header
  margin-left:8px
.skill ul
  margin:0
  margin-top:7px
  margin-left: -8px
.skill ul li
  display: inline-block
  width: 18px
  height: 18px
  border-radius: 50%
.skill ul li:not(:first-child)
  margin-left: 7.5px


.experience-position-degree
  float: left
  display: inline-block

 .experience-period
  display: inline-block
  margin-left: 16px
  clear: right

.education .experience-position-degree
  font-family: 'Myriad Pro Bold'
  color: #3d6cb0
.experience .experience-facility
  font-family: 'Myriad Pro Bold'
.about-header
  position: absolute
  margin-left: 10px
  display:inline-block
  float: left
  top:11px
  text-transform: uppercase
  color: #0071bb
  font-family: 'Myriad Pro Bold'

.about-header::after
  content: ""
  position: absolute
  display: inline-block;
  height: 5px
  background-image: linear-gradient(#0071bb,#0071bb);
  width: 100%
  top: 23px
  margin-left:7px

.about-info
  display: block
  clear: both

span.profile-info-bold,
.contact-info-header
  font-family: 'Myriad Pro Bold'

ul
 list-style: none

li
  margin: 0
  position: relative
.experience li:before
  left:-0.5em
  top: -50px
  position: absolute
  float: left
  content:'•'
  font-size:100px
  vertical-align: top
  margin: 0
  display: inline-block

.education li:before
  left:-0.5em
  top: -50px
  position: absolute
  float: left
  content:'•'
  font-size:100px
  vertical-align: top
  margin: 0
  display: inline-block

.software-skill
  width: 287px
  height: 23px
  background-color: #e6e6e6
  margin: 10px
  padding: 0
  position: relative

.software-skill-colored
  position: absolute
  top: 0
  left:0
  height: 23px
  background-color: #3d6cb0
